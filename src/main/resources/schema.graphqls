input ToDoInput {
    title: String!
    description: String
#    subTasks : [SubTask]
}

type ToDo {
    id: ID!
    title: String!
    description: String
    subTasks : [SubTask]
}

type SubTask {
    id: ID!
    title: String!
    description: String
}

type Query {
    retrieveToDo(id:ID):ToDo!
}

type Mutation {
    createToDo(toDoInput:ToDoInput!):Boolean!
}

schema {
    query: Query
    mutation: Mutation
}